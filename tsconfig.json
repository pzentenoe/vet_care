{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@hooks/*": [
        "src/hooks/*"
      ],
      "@forms/*": [
        "src/components/forms/*"
      ],
      "@layouts/*": [
        "src/layouts/*"
      ],
      "@services/*": [
        "src/services/*"
      ],
      "@stores/*": [
        "src/stores/*"
      ],
      "@styles/*": [
        "src/styles/*"
      ],
      "@utils/*": [
        "src/utils/*"
      ],
      "@lib/*": [
        "src/lib/*"
      ],
      "@assets/*": [
        "src/assets/*"
      ]
    },
    "jsx": "preserve",
    "jsxImportSource": "solid-js"
  }
}